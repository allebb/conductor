#!/usr/bin/env bash

# Load some configuration and 'common' code in...
BINDIR=/etc/conductor/bin
source $BINDIR/conf/conductor.conf
source $BINDIR/_headers

# Application specific....
echo ""
echo "Applications currently deployed:-"
echo "================================="
if [[ -n $(ls -A $APP_ROOT) ]]; then
    ls $APP_ROOT
    echo ""
    exit 0
else
    echo "No applications have yet been deployed on this server!"
    echo ""
    exit 1
fi